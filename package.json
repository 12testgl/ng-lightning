{
  "name": "ng-lightning-app",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "ng": "ng",
    "pug": "gulp pug",
    "pug:watch": "gulp pug:watch",
    "lib:watch": "ng build ng-lightning --watch",
    "app:serve": "ng serve --port 1111",
    "prestart": "rimraf dist && yarn pug",
    "start": "run-p -r pug:watch app:serve",
    "build": "yarn lib:build && yarn demo:build",
    "prelib:build": "rimraf dist/ng-lightning && yarn pug",
    "lib:build": "ng build ng-lightning",
    "predemo:build": "rimraf dist/ng-lightning-app && yarn pug",
    "demo:build": "ng build --prod --base-href=/ng-lightning/",
    "pretest": "yarn pug",
    "test": "ng test ng-lightning --watch=false --code-coverage",
    "lib:tdd": "ng test ng-lightning --source-map=false --watch=true",
    "pretdd": "yarn pug",
    "tdd": "run-p -r pug:watch lib:tdd",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "yarn test && yarn lib:build",
    "release": "node scripts/release",
    "demo:release": "yarn demo:build && node scripts/gh-pages"
  },
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.3",
    "date-fns": "^2.0.0-alpha.27",
    "rxjs": "~6.4.0",
    "semver": "^5.6.0",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "^0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@salesforce-ux/design-system": "^2.8.3",
    "@stackblitz/sdk": "^1.3.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "app-root-path": "^2.2.0",
    "codelyzer": "^4.5.0",
    "conventional-changelog-cli": "^2.0.12",
    "del": "^3.0.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-data": "^1.3.1",
    "gulp-pug": "^4.0.1",
    "inquirer": "^6.2.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^2.0.2",
    "markdown-it": "^8.4.2",
    "ng-packagr": "^4.7.1",
    "npm-run-all": "^4.1.5",
